  "scripts": {
    "compile": "yarn compile:esm && yarn compile:cjs",
    "compile:esm": "tsc",
    "compile:cjs": "tsc --module commonjs --outDir lib/cjs",
    "clean": "rimraf dist lib",
    "copy-files": "copyfiles src/**/**/*.scss src/**/*.scss lib/esm/",
    "build": "rollup -c rollup.config.js",
    "rollup": "yarn clean && yarn compile && yarn copy-files && yarn build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare-launch": "node .launch.js $1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.9",
    "@storybook/builder-webpack5": "^6.2.9",
    "@types/randomcolor": "^0.5.6",
    "@types/react-transition-group": "^4.4.2",
    "@types/styled-components": "^5.1.12",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "classnames": "^2.3.1",
    "postcss": "^8.3.6",
    "randomcolor": "^0.6.2",
    "styled-components": "^5.3.0"
  },
  "files": [
    "/dist",
    "/lib"
